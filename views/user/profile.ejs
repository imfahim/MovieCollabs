<%- contentFor('body') %>
<div class="container">
  <div class="row">
    <div class="col-md-4">
      <div class="panel panel-default">
      <div class="panel-heading">My List</div>
      <div class="panel-body">
        <ul>
          <% list.forEach((movie) => { %>
            <li>
              <a href="/movie/<%= movie.movie_id %>"><span class="glyphicon glyphicon-film"></span> <%= movie.title %></a>
            </li>
          <% }); %>
        </ul>
      </div>
    </div>
</div>
</div>





</div>
